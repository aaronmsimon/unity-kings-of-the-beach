.menu-list-selection {
    -unity-font-style: bold;
    font-size: 36px;
    color: rgb(255, 255, 255);
}

.center-parent {
    width: 100%;
    height: 100%;
    top: 50%;
    left: 50%;
    translate: -50% -50%;
    position: absolute;
    justify-content: center;
    align-items: center;
}

.menu-selected {
    color: rgb(255, 255, 0);
}
